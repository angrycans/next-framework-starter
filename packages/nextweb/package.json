{
  "name": "nextweb",
  "version": "1.0.0",
  "description": "nextweb server",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon server/index.ts",
    "build": "next build src && tsc --project tsconfig.server.json",
    "build:release": "NODE_ENV=production next build ./src && tsc --project tsconfig.server.json",
    "start": "next start",
    "server": "cross-env NODE_ENV=production node build/production-server/server/index.js",
    "analyze:bundles": "cross-env ANALYZE=BUNDLES next build src",
    "analyze:size": "cross-env ANALYZE=SIZE next build src",
    "clean": "rm -rf ./build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "loadtest": "loattest http://localhost:3000 -n 200"
  },
  "author": "",
  "license": "ISC",
