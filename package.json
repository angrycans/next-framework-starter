{
  "name": "next-framework-starter",
  "version": "0.0.1",
  "description": "nextjs mbox typescript ssr ",
  "main": "index.js",
  "scripts": {
    "dev": "next src",
    "dev:server": "nodemon server/index.ts",
    "build": "next build src && tsc --project tsconfig.server.json",
    "build:release": "NODE_ENV=production next build ./src && tsc --project tsconfig.server.json",
    "server": "cross-env NODE_ENV=production node build/production-server/server/index.js",
    "clean": "rm -rf ./build ./.next  ./src/**/*.js  ./src/**/*.js.map ./src/**/*.jsx ./src/**/*.jsx.map",
    "analyze:bundles": "cross-env ANALYZE=BUNDLES next build src"
  },
  "keywords": [
    "nextjs",
    "mbox",
    "typescript",
    "ssr"
  ],
  "author": "acans",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@types/next": "^7.0.1",
    "@types/node": "^10.11.7",
    "@types/react": "^16.4.16",
    "@types/react-dom": "16.0.8",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-less": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-source-maps": "0.0.3",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-import": "^1.9.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "classnames": "^2.2.6",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.4",
    "lerna": "^3.4.0",
    "less-loader": "^4.1.0",
    "lost": "^8.3.0",
    "nextjs-export-path-map": "^0.1.2",
    "node-sass": "^4.9.3",
    "nodemon": "^1.18.4",
    "postcss": "^7.0.5",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-css-variables": "^0.11.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.0",
    "postcss-nested": "^4.1.0",
    "postcss-preset-env": "^6.1.0",
    "postcss-reporter": "^6.0.0",
    "postcss-url": "^8.0.0",
    "raw-loader": "^0.5.1",
    "styled-jsx-plugin-sass": "^0.3.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.2",
    "webpack-bundle-analyzer": "^3.0.2"
  },
  "dependencies": {
    "antd": "^3.10.0",
    "http-status": "^1.2.0",
    "kcors": "^2.2.2",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-helmet": "^4.0.0",
    "koa-logger": "^3.2.0",
    "koa-requestid": "^2.0.1",
    "koa-router": "^7.4.0",
    "less": "^3.8.1",
    "mobx": "^5.5.0",
    "mobx-react": "^5.2.8",
    "mobx-state-tree": "^3.5.0",
    "next": "^7.0.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  }
